{
  "name": "react-ts-vitest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "cy:open": "cypress open",
    "em:start": "firebase emulators:start --import=./saved-data --export-on-exit",
    "em:exec": "firebase emulators:exec --import=./saved-data 'npm start'",
    "em:execui": "firebase emulators:exec --ui --import=./saved-data 'npm start'"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-router": "^1.132.33",
    "@tanstack/react-router-devtools": "^1.132.33",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "firebase": "^12.3.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.64.0",
    "tailwindcss": "^4.1.13",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@tanstack/router-cli": "^1.132.33",
    "@tanstack/router-plugin": "^1.132.33",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "cypress": "^15.7.0",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^6.0.0-rc.1",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.4.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  }
}
